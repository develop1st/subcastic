{
  "name": "subcastic",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace @subcastic/api & npm run dev --workspace @subcastic/web",
    "dev:api": "npm run dev --workspace @subcastic/api",
    "dev:web": "npm run dev --workspace @subcastic/web",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  }
}
